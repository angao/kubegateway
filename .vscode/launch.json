	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Package",
			"type": "go",
			"request": "launch",
			"mode": "debug",
			"program": "${workspaceFolder}/cmd/kube-gateway/main.go",
			"cwd": "${workspaceFolder}",
			"args": [
				"--secure-port=8443",
				"--v=6",
				"--log-dir=output",
				"--logtostderr=false",
				"--enable-proxy-access-log=true",
				"--loopback-client-token=kube-gateway-privileged"
			]
		}
	]
}
